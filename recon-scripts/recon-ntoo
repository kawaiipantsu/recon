#!/usr/bin/env php
<?PHP
/*************************************************************
 * NTOORECON - Number To Operator Reconnaissance
 *
 * Sorry but this is a OSINT reconnaissance script that only
 * works with DANISH / DK phone numbers (Any number that can
 * be operator ported). I will explain in more details in
 * the txt file so that you might find a way to archive the
 * same result for your country.
 *
 * But you can use this tool to look up any danish mobile
 * number and find out what Tele operator it belongs to.
 * Despite it might have been moved between operators since
 * it's origin. 
 *
 * References:
 * - http://och.dk/da/och/om-och.aspx
 * - http://och.dk/media/13721/och_pixi_bog_version_1.0_engelsk.pdf
 *
 * Read about this script in: docs/ctrecon.txt
 *************************************************************/
// Not CLI ? Let's poo out!
if (PHP_SAPI != "cli") exit();

// This is just a random Telco provider i found that exposes the CPS api to some extent.
// They don't show names or nothing important, but they do show the CPS number :) We have the rest!
$telco_api_url = "https://www.telmore.dk/api/common/npimport/validate/mobilenumber/";

// OCH CPS data is Base64 encoded Serialized Array. This is just the data scraped around the internet and refined with pretty names and more info.
// This data should be good for many years - If you want to know what the extra data means, look in the txt file.
$och_packed_data = "";
$cps_data = unserialize(base64_decode($och_packed_data));

// Let's always show off the not so gay figlet text!
echo "     ____  _   _ _____ ___   ___  ____  _____ ____ ___  _   _  "."\n";
echo "    / / / | \ | |_   _/ _ \ / _ \|  _ \| ____/ ___/ _ \| \ | | KawaiiPantsu"."\n";
echo "   / / /  |  \| | | || | | | | | | |_) |  _|| |  | | | |  \| | (c) 2020"."\n";
echo "  / / /   | |\  | | || |_| | |_| |  _ <| |__| |__| |_| | |\  | "."\n";
echo " /_/_/    |_| \_| |_| \___/ \___/|_| \_\_____\____\___/|_| \_| "."\n";
echo " "."\n";
echo " {         (Phone) Number To Operator Reconnaissance         }"."\n";
echo " {    Taking advandtage of OCH's data and Telco's API's      }"."\n";

// Let's utilize the getopt thinggy!
$shortopts = "n:hl";
$longopts  = array("number:","help","listoperators","csv");
$options = getopt($shortopts, $longopts);

// Default values
$number = false;
$csv=false;

foreach ( $options as $option => $value ) {
 switch ( $option ) {
  case "n":
   $number = trim($value);
   break;
  case "number":
   $number = trim($value);
   break;
   case "l":
    listopers();
    break;
   case "listoperators":
    listopers();
    break;
   case "csv":
     $csv=true;
    break;
   case "h":
   help();
   break;
  case "help":
   help();
   break;
 }
}

// Sanity check
if ($number === false) help();   // Forgot domain
if (strlen($number) < 8) help(); // This is not likely ?

if ( $lookup_data = @cpsinfolookup($number) ) {
  if ( $lookup_data["httpcode"] == "200" ) {
    
    $details_full = array(); // This will hold all the info when we are done!

    $cps_info = json_decode($lookup_data["response"],true);
    $cps_id = trim($cps_info["teleOperatorDTO"]["cps"]);

    $simstatus = false;

    if ( isset($cps_info["teleOperatorDTO"]["iccLength"]) && $cps_info["teleOperatorDTO"]["iccLength"] != "") {
      $cps_icc_len    = trim($cps_info["teleOperatorDTO"]["iccLength"]);
      $simstatus = true;
    } else $cps_icc_len = false;

    if ( isset($cps_info["teleOperatorDTO"]["iccPrefix"]) && $cps_info["teleOperatorDTO"]["iccPrefix"] != "" ) {
      $cps_icc_prefix = trim($cps_info["teleOperatorDTO"]["iccPrefix"]);
      $simstatus = true;
    } else $cps_icc_prefix = false;

    $details_full["phone_number"]   = $number;

    if ( $simstatus ) $details_full["number_type"] = "mobile";
    else $details_full["number_type"]              = "fixed";

    $details_full["sim_card"]       = $simstatus;
    $details_full["sim_icc_len"]    = $cps_icc_len;
    $details_full["sim_icc_prefix"] = $cps_icc_prefix;

    // Let's combind data and build all info we want to show on the Number

    foreach ($cps_data as $cps ) {
      if ( $cps["cps"] == $cps_id) {
        $details_full["operator_name"] = trim($cps["name"]);
        $details_full["operator_id"]   = trim($cps["id"]);
        $details_full["operator_cps"]  = trim($cps["cps"]);
       
        $details_full["operator_alt_names"] = $cps["alternative_names"];

        if ( $cps["networkoperator"] )     $oper_type = "Network Operator";
        elseif ( $cps["serviceoperator"] ) $oper_type = "Service Operator";
        else                               $oper_type = "Unknown";

        $details_full["operator_type"]   = $oper_type;
        $details_full["och_direct_conn"] = trim($cps["och_direct_conn"]);
        
        break;
      }
    }

    // We got all the output now let's present it somewhat neato!!
    // Let's keep it to 80 chars for retro fixed consoles .. They do exist!
    
    echo "\n";
    echo "-// Looks like it all went okay! Here is what i found ...\n";
    echo "\n";
    
    echo "-// Number ..........: $number\n";
    echo "-// Number type .....: ".$details_full["number_type"]."\n";
    echo "-// \n";
    echo "-// Operator Name ...: ".$details_full["operator_name"]."\n";
    echo "-// Operator Type ...: ".$details_full["operator_type"]."\n";
    echo "-// \n";
    $aliases = trim(rtrim(implode(",",$cps["alternative_names"]),","));
    if ($aliases == "") $aliases = "None";
    echo "-// Operator aliases : ".$aliases."\n";
    echo "-// \n";
    echo "-// Operator ID .....: ".$details_full["operator_id"]."\n";
    echo "-// OCH CPS Number ..: ".$details_full["operator_cps"]."\n";
    if ( $details_full["och_direct_conn"] ) $och = "Yes";
    else $och = "No";
    echo "-// OCH Direct Conn .: ".$och."\n";
    echo "-// \n";
    if ( $details_full["sim_card"] ) $sim = "Yes";
    else $sim = "No";
    echo "-// SIM Card ........: ".$sim."\n";
    echo "-// SIM ICC Length ..: ".$details_full["sim_icc_len"]."\n";
    echo "-// SIM ICC Prefix ..: ".$details_full["sim_icc_prefix"]."\n";
    echo "\n";

  } else {
    echo "\n";
    echo "-// Seems like there was a problem parsing the json data.. o.O\n";
    echo "\n";  
  }
} else {
  echo "\n";
  echo "-// Seems like there was a problem getting the data.. o.O\n";
  echo "-// Error: ".trim($lookup_data["error_msg"])."\n";
  echo "\n";
}

exit(); // We done !



// Functions

function listopers() {
  global $cps_data;
  global $csv;

  echo "\n";
  echo "-// I know a total of ".count($cps_data)." main operators in Denmark (DK)!\n";
  echo "-// Some of them might even have alternative operator names...\n";
  echo "\n";

  if ($csv) {
    printf("ID;CPS;Operator Name;Operator Type;OCH;Alternative names\n");
    foreach ($cps_data as $cps) {
      printf("%s;%s;%s;%s;%s;%s\n",$cps["name"],$cps["id"],$cps["cps"],"a","b","c");
    }
  } else {
    printf("-ID----|-CPS----|-Type----|-OCH-|-Operator Name------------------|-Alternative names----------\n");
    printf("       |        |         |     |                                |\n");
    foreach ($cps_data as $cps) {
      if ( $cps["och_direct_conn"] ) $och = "Yes";
      else $och = "No";
      if ( $cps["networkoperator"] ) $type = "NETWORK";
      else if ( $cps["serviceoperator"] ) $type = "SERVICE";
      else $type = "UNKNOWN";
      $alt_names = trim(rtrim(implode(",",$cps["alternative_names"]),","));
      if ( mb_strlen($cps["name"]) != strlen($cps["name"]) ) {
        // Find out padding
        $lenpad = strlen($cps["name"]) - mb_strlen($cps["name"]);
        printf(" %5s | %7s| %7s | %3s | %".(30+$lenpad)."s | %s\n",$cps["id"],$cps["cps"],$type,$och,$cps["name"],$alt_names);
      } else {
        printf(" %5s | %7s| %7s | %3s | %30s | %s\n",$cps["id"],$cps["cps"],$type,$och,$cps["name"],$alt_names);
      }

    }
    printf("\n");
  }

  exit();
}

function help() {
    printf("\n");
    printf("IMPORTANT: This for now only works on Danish (DK) phone numbers.\n");
    printf("           Works on most types of numbers, fast, mobile, voip\n");
    printf("\n");
    printf("Usage:\n");
    printf("\n");
    printf(" -n, --number        :: Phone number to lookup Operator\n");
    printf(" -l, --listoperators :: List all Operators known to me\n");
    printf(" -h, --help          :: Show this help ...\n");
    printf("\n");
    printf("Read about this script in:\n");
    printf("\n".str_replace("/recon-scripts","",__DIR__)."/docs/ntoorecon.txt\n");
    printf("\n");
    printf("\n");
    exit();
   }

function cpsinfolookup($n=false) {
    global $telco_api_url;
    if ($n === false) return false;
    $url = $telco_api_url.$n;
   
    $ch = curl_init($url);
    curl_setopt($ch, CURLOPT_RETURNTRANSFER,  true);
    curl_setopt($ch, CURLOPT_AUTOREFERER, true);
    curl_setopt($ch, CURLOPT_USERAGENT, "Mozilla/5.0 (Windows NT 6.1; WOW64) AppleWebKit/537.36 (KHTML like Gecko) Chrome/36.0.1985.143 Safari/537.36");
    curl_setopt($ch, CURLOPT_ENCODING, "gzip,deflate");
    curl_setopt($ch, CURLOPT_SSL_VERIFYHOST, false);
    curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false);
    curl_setopt($ch, CURLOPT_FOLLOWLOCATION,  true);
    $result = array();
    $result["response"] = curl_exec($ch);
    $result["httpcode"] = trim(curl_getinfo($ch, CURLINFO_HTTP_CODE));
    if ( curl_error($ch) ) {
     $result["error_msg"] = trim(curl_error($ch));
    }
    curl_close($ch);
    return $result;
}

?>